{
	"name": "alex",
	"version": "0.1.1",
	"displayName": "alex",
	"description": "Find gender favouring, race related, religion inconsiderate or other unequal phrasing",
	"publisher": "shinnn",
	"repository": "https://github.com/shinnn/vscode-alex",
	"homepage": "https://github.com/shinnn/vscode-alex#readme",
	"bugs": "https://github.com/shinnn/vscode-alex/issues",
	"qna": "https://stackoverflow.com/questions/tagged/vscode+alex",
	"license": "ISC",
	"icon": "media/icon.png",
	"galleryBanner": {
		"color": "#FAFAFA",
		"theme": "light"
	},
	"engines": {
		"vscode": ">=1.27.0"
	},
	"categories": [
		"Linters"
	],
	"activationEvents": [
		"onLanguage:plaintext",
		"onLanguage:markdown"
	],
	"main": "index.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "alex configuration options",
			"properties": {
				"alex.enable": {
					"type": "boolean",
					"default": false,
					"description": "Control whether alex automatically check files or not."
				}
			}
		}
	},
	"scripts": {
		"test": "eslint ."
	},
	"dependencies": {
		"alex-vscode": "^2.0.0-0",
		"vscode-languageclient": "^5.1.0",
		"vscode-languageserver": "5.1.0"
	},
	"devDependencies": {
		"@shinnn/eslint-config": "^6.7.1",
		"eslint": "^5.6.0",
		"vscode": "^1.1.21"
	},
	"eslintConfig": {
		"extends": "@shinnn"
	}
}
